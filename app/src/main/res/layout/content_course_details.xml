<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_business"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.hackathon.jdccollege.model.AttractionListTab"
    android:background="@drawable/business_background_deatils2">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:weightSum="1">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_chrome_reader_mode_black_24dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_marginLeft="12dp"
                android:text="Name"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/idTV1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:text=""
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAllCaps="false"
            android:textAlignment="center" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2">

            <ImageView
                android:layout_width="33dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_assignment_black_48dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Degree"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/nameTV2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2">

            <ImageView
                android:layout_width="37dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_hourglass_full_black_48dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Duration"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/addressTV3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2"
            android:weightSum="1">

            <ImageView
                android:layout_width="37dp"
                android:layout_height="35dp"
                app:srcCompat="@drawable/ic_show_chart_black_48dp" />

            <TextView
                android:layout_width="48dp"
                android:layout_height="match_parent"
                android:text="Frequecy"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/emailTV4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="36dp"
                app:srcCompat="@drawable/ic_attach_money_black_48dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Cost"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/websiteTV5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2"
            android:weightSum="1">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                app:srcCompat="@drawable/ic_history_black_48dp" />

            <TextView
                android:layout_width="66dp"
                android:layout_height="wrap_content"
                android:text="PreCourses"
                android:textSize="9sp"
                android:textAlignment="center"
                android:gravity="left"/>
        </LinearLayout>

        <TextView
            android:id="@+id/websiteTV6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2">

            <ImageView
                android:layout_width="33dp"
                android:layout_height="34dp"
                app:srcCompat="@drawable/ic_location_on_black_48dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Area"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/websiteTV7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="2">

            <ImageView
                android:layout_width="36dp"
                android:layout_height="35dp"
                app:srcCompat="@drawable/ic_info_black_48dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Website"
                android:textSize="9sp"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:id="@+id/websiteTV8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="sans-serif-smallcaps"
            android:typeface="monospace"
            android:textSize="24sp"
            android:lineSpacingExtra="36sp"
            android:layout_weight="8"
            android:textAlignment="center" />
    </LinearLayout>
</LinearLayout>
